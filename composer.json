{
  "name": "torann/laravel-repository",
  "description": "Base repository implementation for Laravel",
  "keywords": [
    "laravel",
    "repository",
    "repository pattern",
    "eloquent",
    "repositories"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^7.0|^8.0|^9.0",
    "illuminate/database": "^7.0|^8.0|^9.0",
    "illuminate/cache": "^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.3",
    "phpstan/phpstan": "^0.12.14",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Torann\\LaravelRepository\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/TestFunctions.php"
    ],
    "psr-4": {
      "Torann\\LaravelRepository\\Test\\": "tests"
    }
  },
  "archive": {
    "exclude": [
      "/tests"
    ]
  }
}
