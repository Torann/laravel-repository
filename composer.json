{
  "name": "torann/laravel-repository",
  "description": "Base repository implementation for Laravel",
  "keywords": [
    "laravel",
    "repository",
    "repository pattern",
    "eloquent",
    "repositories"
  ],
  "license": "BSD 2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "daniel@lyften.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.1",
    "illuminate/database": "~5.1",
    "illuminate/cache": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Torann\\LaravelRepository\\": "src/Torann/LaravelRepository"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },
  "archive" : {
    "exclude": [
      "/tests"
    ]
  }
}
